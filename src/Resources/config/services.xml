<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <!-- Controller -->
        <service id="Myfav\Mig\Storefront\Controller\SetupController" public="true">
            <argument type="service" id="Myfav\Mig\Service\MyfavAuthService"/>
            <argument type="service" id="Myfav\Mig\Service\MyfavMigService"/>
            <argument type="service" id="Symfony\Component\Routing\RouterInterface" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <call method="setTwig">
                <argument type="service" id="twig"/>
            </call>
        </service>

        <service id="Myfav\Mig\Storefront\Controller\WorkerController" public="true">
            <argument type="service" id="Myfav\Mig\Service\MyfavAuthService"/>
            <argument type="service" id="Myfav\Mig\Service\MyfavMigService"/>
            <argument type="service" id="Symfony\Component\Routing\RouterInterface" />
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <call method="setTwig">
                <argument type="service" id="twig"/>
            </call>
        </service>

        <!-- Entity -->
        <service id="Myfav\Mig\Core\Content\MyfavMig\MyfavMigDefinition">
            <tag name="shopware.entity.definition" entity="myfav_mig" />
        </service>

        <!-- Processor -->
        <service id="Myfav\Mig\Processor\ProductProcessorOne" public="true">
            <argument id="Myfav\Mig\Service\ApiService" type="service"/>
            <argument id="Myfav\Mig\Service\CategoryService" type="service"/>
            <argument id="Myfav\Mig\Service\ManufacturerService" type="service"/>
            <argument id="Myfav\Mig\Service\MyfavMigService" type="service"/>
            <argument id="Myfav\Mig\Service\ProductService" type="service"/>
            <argument id="Myfav\Mig\Service\PropertyService" type="service"/>
            <argument id="Shopware\Core\System\SystemConfig\SystemConfigService" type="service"/>
        </service>

        <!-- Service -->
        <service id="Myfav\Mig\Service\CategoryService" public="true">
            <argument id="category.repository" type="service"/>
        </service>

        <service id="Myfav\Mig\Service\ApiService" public="true">
            <argument id="Shopware\Core\System\SystemConfig\SystemConfigService" type="service"/>
        </service>

        <service id="Myfav\Mig\Service\ManufacturerService" public="true">
            <argument id="product_manufacturer.repository" type="service"/>
        </service>

        <service id="Myfav\Mig\Service\MyfavAuthService" public="true">
            <argument id="Shopware\Core\System\SystemConfig\SystemConfigService" type="service"/>
        </service>

        <service id="Myfav\Mig\Service\ProductService" public="true">
            <argument id="product.repository" type="service"/>
        </service>

        <service id="Myfav\Mig\Service\PropertyService" public="true">
            <argument id="product.repository" type="service"/>
            <argument id="property_group.repository" type="service"/>
            <argument id="property_group_option.repository" type="service"/>
        </service>

        <service id="Myfav\Mig\Service\MyfavMigService" public="true">
            <argument id="myfav_mig.repository" type="service"/>
        </service>
    </services>
</container>
